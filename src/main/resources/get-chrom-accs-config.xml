<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:int="http://www.springframework.org/schema/integration"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/integration http://www.springframework.org/schema/integration/spring-integration.xsd">

    <int:channel id="channelIntoGetChrom"/>

    <bean id="processSequenceReport" class="embl.ebi.variation.eva.sequence_report_processing.ProcessSequenceReport"/>
    <int:transformer input-channel="channelIntoGetChrom" ref="processSequenceReport" method="transform"/>









</beans>